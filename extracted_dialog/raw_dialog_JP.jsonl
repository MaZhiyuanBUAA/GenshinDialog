[{"role": "ノート", "nextDialogs": [3470202], "content": "「友人に呑みに誘われたため、しばらく休業する。店主より」", "id": 3470201}, {"role": "パイモン", "nextDialogs": [], "content": "モンドの人はそんなにお酒が好きなのか？", "id": 3470202}]
[{"role": "パイモン", "nextDialogs": [], "content": "冒険者協会は人が足りないのか？", "id": 3470304}, {"role": "求人掲示板", "nextDialogs": [3470302], "content": "「応募者はここで自己紹介、応募希望と連絡先を残してください。」", "id": 3470301}, {"role": "求人掲示板", "nextDialogs": [3470303], "content": "「雇用者はほしい人材と連絡先を残してください。」", "id": 3470302}, {"role": "パイモン", "nextDialogs": [3470304], "content": "…どうして全部「冒険者協会、モンド支部」からの求人情報しかない？", "id": 3470303}]
[{"role": "図書館ルール", "nextDialogs": [3470402], "content": "「図書館ルール·Ver.7」", "id": 3470401}, {"role": "図書館ルール", "nextDialogs": [3470403], "content": "「以前もルールをたくさん作ったけど、効果は見られなかった。」", "id": 3470402}, {"role": "図書館ルール", "nextDialogs": [3470404], "content": "「今回は必要でないルールを全てなしにしてみた。図書館を利用する者は以下のルールだけ覚えてちょうだいね：」", "id": 3470403}, {"role": "図書館ルール", "nextDialogs": [3470405], "content": "「1.図書館の中は静かに。」", "id": 3470404}, {"role": "図書館ルール", "nextDialogs": [3470406], "content": "「2.図書館の所有物を壊さない。」", "id": 3470405}, {"role": "図書館ルール", "nextDialogs": [3470407], "content": "「3.借りたものはちゃんと期間内に返す。」", "id": 3470406}, {"role": "図書館ルール", "nextDialogs": [3470408], "content": "「ルールを破ったらどうなるかみんな分かってると思うから、ここは割愛させてもらうわ。」", "id": 3470407}, {"role": "図書館ルール", "nextDialogs": [], "content": "「以上。みんなで楽しく図書館を使いましょう。」", "id": 3470408}]
[{"role": "ノート", "nextDialogs": [3470502], "content": "「飲みに行った。しばらく休業する。店主より」", "id": 3470501}, {"role": "パイモン", "nextDialogs": [], "content": "モンドの人は本当にお酒が好きだな…", "id": 3470502}]
[{"role": "掲示板", "nextDialogs": [3470602], "content": "「キノコはいいものである。」", "id": 3470601}, {"role": "掲示板", "nextDialogs": [3470603], "content": "「キノコピザでも、松茸のバター焼きでも、鳥肉と野生キノコの串焼きでも、キノコがないと成り立たない…」", "id": 3470602}, {"role": "掲示板", "nextDialogs": [3470604], "content": "「その中の栄養素も言わずもがな！まさに美食の中の美食！」", "id": 3470603}, {"role": "掲示板", "nextDialogs": [3470605], "content": "「これを読んでいるあなたにキノコの良さを知ってもらうために、掲示板の横にキノコを置いておいたから、どうぞ。」", "id": 3470604}, {"role": "掲示板", "nextDialogs": [], "content": "「キノコを気に入ってくれると嬉しい。」", "id": 3470605}]
[{"role": "ノート", "nextDialogs": [3470702], "content": "「閑散季のため、休業中。」", "id": 3470701}, {"role": "ノート", "nextDialogs": [], "content": "「私に用事がある者は璃月に来てください。茶博士劉蘇より」", "id": 3470702}]
[{"role": "掲示板", "nextDialogs": [3470802], "content": "「夏限定のグレープジュース、2本で1モラ」", "id": 3470801}, {"role": "パイモン", "nextDialogs": [3470803], "content": "なんて安い！", "id": 3470802}, {"role": "掲示板", "nextDialogs": [3470804], "content": "「品切れ、毎度あり」", "id": 3470803}, {"role": "パイモン", "nextDialogs": [], "content": "分かった…", "id": 3470804}]
[{"role": "掲示板", "nextDialogs": [3470902], "content": "「スタッフ休憩所」", "id": 3470901}, {"role": "掲示板", "nextDialogs": [], "content": "「お疲れ様。お酒と軽食をご自由にどうぞ。」", "id": 3470902}]
[{"role": "フクロウの彫像", "nextDialogs": [3471102], "content": "「闇夜を守る者よ、暁の光を忘れるな」", "id": 3471101}, {"role": "フクロウの彫像", "nextDialogs": [], "content": "「邪佞と戦う者よ、正直な心を忘れるな」", "id": 3471102}]
[{"role": "明蘊町掲示板", "nextDialogs": [3471302, 3471303, 3471304], "content": "「中原のもつ焼きと二さんとの不幸な事件を鑑みて、ここの掲示板を再開しようと決めた。今後共有したいことがあったら、ここでメッセージを残すといい。」", "id": 3471301}, {"role": "Traveller", "nextDialogs": [3471305], "content": "最新のメッセージを閲覧する…", "id": 3471302}, {"role": "Traveller", "nextDialogs": [3471306], "content": "古いメッセージを閲覧する…", "id": 3471303}, {"role": "Traveller", "nextDialogs": [], "content": "離れる", "id": 3471304}, {"role": "明蘊町掲示板", "nextDialogs": [3471303, 3471304], "content": "「上の手配によって、来月から層岩巨淵で働くことになった。前の鉱山主の宝物伝説に関する妄想もそろそろ終わりにして、地道に働くことこそが正道だ。二さんの教訓を忘れないように。」", "id": 3471305}, {"role": "明蘊町掲示板", "nextDialogs": [3471307], "content": "「この間見つけた前の鉱山主の鴻爺さんの遺訓、家書と宝物の伝説に関して、みんなあまり本気になりすぎないように。進捗を遅らせてはいけない。地道に働くことこそが正道だ。」", "id": 3471306}, {"role": "明蘊町掲示板", "nextDialogs": [3471308, 3471304], "content": "「それから、近頃荻花洲の辺りに盗賊が出没しているため、今月の行商人はいつもより遅れる可能性がある。」", "id": 3471307}, {"role": "Traveller", "nextDialogs": [3471331], "content": "引き続き読む…", "id": 3471308}, {"role": "明蘊町掲示板", "nextDialogs": [3471310], "content": "「ヤマガラが足りない件について、新しいの購入する手配をしておいた。それから、二つ表明したいことがある：」", "id": 3471309}, {"role": "明蘊町掲示板", "nextDialogs": [3471311], "content": "「一つ目、ヤマガラは採掘の際に鉱坑内の空気が安全かどうかを区別する大事な役割がある。」", "id": 3471310}, {"role": "明蘊町掲示板", "nextDialogs": [3471312], "content": "「二つ目、ヤマガラを食事にしないでほしい。」", "id": 3471311}, {"role": "明蘊町掲示板", "nextDialogs": [3471313, 3471304], "content": "「三つ目、二さんと中原のもつ焼きの教訓を忘れないよに。」", "id": 3471312}, {"role": "Traveller", "nextDialogs": [3471314], "content": "引き続き読む…", "id": 3471313}, {"role": "明蘊町掲示板", "nextDialogs": [3471315], "content": "「みんな！主屋の遺訓の件はまだ覚えてるか？今日鉱坑内で手がかりを見つけたんだ。中に紅葉とかなんとか書いてある。さすが鴻爺さん、文化人の言うことは分からないや！二さんがまだここにいたらいいのに！」", "id": 3471314}, {"role": "明蘊町掲示板", "nextDialogs": [3471316, 3471304], "content": "「忠、時間をそんなものに使うな。」", "id": 3471315}, {"role": "Traveller", "nextDialogs": [3471317], "content": "引き続き読む…", "id": 3471316}, {"role": "明蘊町掲示板", "nextDialogs": [3471318], "content": "「近頃鉱石量が減った件について、戴と話しあったが、明蘊町の鉱物資源はもうすぐ空っぽになる可能性がある。」", "id": 3471317}, {"role": "明蘊町掲示板", "nextDialogs": [3471319, 3471304], "content": "「輝山庁の人とも話しあった。もうしばらくしたら、層岩巨淵で働くことになるかもしれない。心配は不要だ。」", "id": 3471318}, {"role": "Traveller", "nextDialogs": [3471320], "content": "引き続き読む…", "id": 3471319}, {"role": "明蘊町掲示板", "nextDialogs": [3471321], "content": "「戴へ：『砕夢奇珍』最新刊がどうしても入手できないんだ。あとでまた見せるよ。」", "id": 3471320}, {"role": "明蘊町掲示板", "nextDialogs": [3471322], "content": "「二さんへ：今回会えなかったから、二さんが注文したキノコ、キャベツ、チーズ、小麦粉とレシピは忠さんに渡しておいた。彼から受け取ってね。」", "id": 3471321}, {"role": "明蘊町掲示板", "nextDialogs": [3471323], "content": "「みんな！今夜は俺の奢りでキノコピザだ！」", "id": 3471322}, {"role": "明蘊町掲示板", "nextDialogs": [3471324, 3471304], "content": "キノコピザ！", "id": 3471323}, {"role": "Traveller", "nextDialogs": [3471325], "content": "引き続き読む…", "id": 3471324}, {"role": "明蘊町掲示板", "nextDialogs": [3471326], "content": "「最近鉱坑内で鴻爺さんが残したものを見つけた。兄弟とか書いてある。主屋に見つけた爺さんの遺訓に書いてある宝物と関係があるかもしれない。」", "id": 3471325}, {"role": "明蘊町掲示板", "nextDialogs": [3471327, 3471304], "content": "「しかし宝探しよりも安全が第一だ。採掘も同じ、たまに頭の上の石を叩くんだ。もし中は空っぽのような音があったら、崩れる可能性があるからな。ついでに二さんに黙禱する。」", "id": 3471326}, {"role": "Traveller", "nextDialogs": [3471328], "content": "引き続き読む…", "id": 3471327}, {"role": "明蘊町掲示板", "nextDialogs": [3471329], "content": "「みんな！今日はここでの最終日だ！来週結婚するんだ。もし時間があったら、結婚式に参加してほしい！」", "id": 3471328}, {"role": "明蘊町掲示板", "nextDialogs": [3471330], "content": "「さらば、明蘊町！さらば、みんな！誰か鴻爺さの宝物を見つけたら、手紙をくれよな！」", "id": 3471329}, {"role": "明蘊町掲示板", "nextDialogs": [], "content": "「忠が幸せになることを祈っている。しかし宝探しに時間を使ってほしくないんだ。我々は鉱夫、冒険者でも宝盗団でもないんだ。」", "id": 3471330}, {"role": "明蘊町掲示板", "nextDialogs": [3471332, 3471304], "content": "「鉱坑内で炎が青くなったら気を付けるんだ。地下の有害ガスの可能性があるから、爆発になると厄介なんだ。ついでに二さんに黙禱する。」", "id": 3471331}, {"role": "Traveller", "nextDialogs": [3471309], "content": "引き続き読む…", "id": 3471332}]
